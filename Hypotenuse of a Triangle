#include <iostream>
#include <cmath>

using namespace std;

int main() 
{
	double x;
	cout << " Enter the value of X =";
	cin >> x;

	double y;
	cout << "Enter the value of Y =";
	cin >> y;

	double hypo = sqrt(pow(x,2) + pow(y,2));

	
	cout << "Hypotenuse of the traiangle is =" << hypo;

	return 0; 

}
